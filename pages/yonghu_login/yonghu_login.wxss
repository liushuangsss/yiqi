.login_img{width: 171rpx;height: 171rpx;}
.login_box{padding: 30rpx;text-align: center;background: #fff;}
.login_list{height: 100rpx;line-height: 100rpx;border-bottom: 1rpx solid #ddd;text-align: left;margin:0 30rpx}
.login_list label{ width: 15%;font-size: 28rpx;}
.login_list input{float: right;width:85%; line-height: 100rpx;height: 100rpx;font-size: 28rpx;}
.login_btn{width: 290rpx;height: 78rpx;line-height: 78rpx;color: #fff;background: #0096E1;text-align: center;border:none;border-radius: 10rpx;font-size: 30rpx;margin-left: 50rpx;}
.togetpsd{font-size: 30rpx;color: #0096E1;position: fixed;bottom: 70rpx;width: 100%;text-align: center}
.register{width: 286rpx;height: 74rpx;border: 2rpx solid #0096E1;line-height: 74rpx;border-radius: 10rpx;font-size: 30rpx;color: #0096E1}
.btn_view{display: flex;flex-direction: row;padding:60rpx 30rpx;}

.fail{position: fixed;top: 0;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.5);}
.fail_center{width: 560rpx;height: 295rpx;margin: 0 auto;background: #fff;margin-top:50%;}
.fail_title{display: inline-block;width: 100%;text-align: center;height: 194rpx;line-height: 194rpx;font-size: 30rpx;font-weight: bold;border-bottom: 1rpx solid #D2D3D5;}
.know{line-height: 77rpx;text-align: center;color: #0096E1;font-size: 36rpx;display: inline-block;width: 100%;}