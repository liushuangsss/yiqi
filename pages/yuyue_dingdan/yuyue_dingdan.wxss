/* pages/yuyue_dingdan/yuyue_dingdan.wxss */
swiper{
  height: 1500rpx;
}
.swiper-tab{
width: 100%;
background: #fff;
border-bottom: 2rpx solid #ccc;
text-align: center;
height: 88rpx;
line-height: 88rpx;
font-weight: bold;
}
.swiper-tab-item{
display: inline-block;
margin:0 50rpx;
}
.active{
  color:#0096E1;
  border-bottom: 4rpx solid #0096E1;
}
.box{
  width:100%;
}
.sou{
  width:100%;
  padding: 14rpx 0;
  text-align: center;
}
.sou input{
  width:720rpx;
  height: 66rpx;
  border-radius: 10rpx;
  background: #fff;
}
.s_box{
  width:100%;
  background: #fff;
  margin-bottom: 20rpx;
}
.s_boxs{
  width:94%;
  margin: auto;
  overflow: hidden;
}
.success{
  width:100%;
  height: 76rpx;
  line-height: 76rpx;
  border-bottom: 1rpx solid #DDDDDD;
}
.chenggong{
  color:#0096E1;
  font-size: 28rpx;
}
.time{
  float: right;
  color: #999999;
  font-size: 26rpx;
}
.chanpin{
  width:100%;
  overflow: hidden;
  padding:20rpx 0;
}
.left{
  float: left;
  width:35%;
  height:259rpx;
}
.left image{
  width:220rpx;
  height:165rpx;
  border: 1rpx solid #DDDDDD;
}
.right{
  float: left;
  width:60%;
}
.zhuangtai{
  font-size: 22rpx;
  color:#9C9C9C;
  height: 60rpx;
  line-height: 60rpx;
  padding-bottom: 20rpx;
}
.times text{
  display: block;
  width:170rpx;
  height: 42rpx;
  line-height: 42rpx;
  border: 1rpx solid #E3E4E6;
  background: #F3F3F3;
  float: left;
  margin-right: 10rpx;
  margin-top: 14rpx;
  text-align: center
}
.times image{
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
  padding-top: 18rpx;
  float: right;
}
.boths{
  clear: both;
}
.errors{
  width:100%;
  border-top: 1rpx solid #DDDDDD;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  color:#999999;
}
.successes{
  text-align: center;
}
.successes image{
  width:376rpx;
  height: 247rpx;
  margin: 115rpx 0;
}
.yuyueco{
  color:#0096E1;
}
.nav{position: fixed;bottom: 0;height: 97rpx;line-height: 97rpx;width: 100%;background: #fff;display: flex;flex-direction: row}
.nav view image{width: 35rpx;height: 34rpx;}
.nav view{display: flex;width: 33%;justify-content: center;align-items: center;flex-direction: column;line-height: 40rpx;}