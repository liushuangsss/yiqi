.shouq_box{background: #fff;padding: 30rpx;font-size: 26rpx;text-align: center}
.shouq_box image{width: 96rpx;height: 125rpx;vertical-align: middle;margin: 0 10rpx;}
.shouq_box text{color:#0096E1; }
.sq_btn{margin: 80rpx 0 30rpx 0;display: flex;flex-direction: row;justify-content: center}
.sq_btn button:first-child{width: 290rpx;height: 79rpx;line-height: 79rpx;background: #808080;color: #fff;border-radius: 8rpx;font-size: 30rpx;margin-right: 20rpx;}
.sq_btn button:last-child{width: 290rpx;height: 79rpx;line-height: 79rpx;background:#0096E1;color: #fff;border-radius: 8rpx; }

.jujue_tc{position: fixed;top: 0;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.5)}

.jujue_cont{width: 560rpx;background: #fff;margin: 0 auto;height: 646rpx;border-radius: 8rpx;margin-top: 20%;text-align: center;position: relative;}
.jujue_title{font-size: 36rpx;display: inline-block;height: 60rpx;line-height: 60rpx;margin-top: 50rpx;font-weight: bold}
.write_info{width: 430rpx;height: 270rpx;background: #ebebeb;margin: 0 auto;text-align: left;padding: 30rpx;margin-top: 30rpx;}
.btn_jujue{position: absolute;bottom: 0;display: flex;flex-direction: row;width: 100%;height:100rpx;line-height: 100rpx;border-top: 1rpx solid #D2D3D5; }
.btn_jujue button{width: 50%;line-height: 100rpx;font-size: 36rpx;}
.success{color: #0096E1;border-left: 1rpx solid #D2D3D5}