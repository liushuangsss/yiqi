.wrap{
  background: #fff;
}
.date-show{
    position: relative;
    width: 250rpx;
    font-family: PingFang-SC-Regular;
    font-size: 30rpx;
    color: #333333;
    text-align: center;
    margin: 20rpx auto 10rpx;
}
.lt-arrow,.rt-arrow{
    position: absolute;
    top: 1rpx;
    width: 60rpx;
    height: 60rpx;
    padding-right: 0.6rem;
}
.lt-arrow image,.rt-arrow image{
    width: 14rpx;
    height: 23rpx;
}
.lt-arrow{
    left: -250rpx;
    /* transform: rotate(180deg); */
}
.rt-arrow{
    right: -270rpx;
}
.header{
    font-size: 0;
    padding: 0 24rpx;
}
.header>view{
    display: inline-block;
    width: 14.285%;
    color: #333;
    font-size: 30rpx;
    text-align: center;
    border-bottom: 1px solid #D0D0D0;
    padding: 39rpx 0;
}
.weekMark{
    position: relative;
}
.weekMark view{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    border-bottom: 1px solid #22A7F6;
}
