.box{
  background: #fff;
}
.swiper-tab{  
    width:75%;  
    text-align: center;  
    line-height: 80rpx;
    display: flex;
    flex-direction: row;
    overflow-x: auto;
    white-space:nowrap;
    margin: 0 auto;
    padding: 30rpx 0;
  }  
.swiper-tab-list{  font-size: 30rpx;  
  display: inline-block;  
   width: 33%;
   text-align: center;
   height: 67rpx;line-height: 67rpx;
   border: 2rpx solid #0096E1;
   color: #0096E1;
} 
.swiper-tab-list:nth-of-type(1){
  border-bottom-left-radius: 10rpx;
  border-top-left-radius: 10rpx;
} 
.swiper-tab-list:nth-of-type(3){
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.on{  
  border: 2rpx solid #0096E1;
  background: #0096E1;
  color: #fff;
}  
.swiper-box{ display: block; min-height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}  

.tab{
  padding-bottom: 200rpx;
}

.tab-left{ 
  width: 100%; 
  line-height: 160%; 
  border-right: solid 1px gray; 
  display: flex;
  flex-direction: row;
} 
.tab-left view{ 
  border-bottom: solid 1px red; 
} 
.tab-left .active{ 
  color: #f00; 
} 
.tab-right{ 
  line-height: 160%; 
} 
.tab-right .right-item{ 
  display: none; 
} 
.tab-right .right-item.active{ 
  display: block; 
  margin-bottom: 90rpx;
  position: relative;
} 


