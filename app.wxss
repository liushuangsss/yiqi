/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
}
::-webkit-scrollbar{display:none;}
image{width:auto;height:auto;}
.right{float:right;}
.left{float:left;}
.clear{clear:both;}
button{padding-left:0;padding-right:0;margin:0;box-sizing:inherit;font-size:24rpx;text-align:center;text-decoration:none;line-height:24rpx;border-radius:unset;border:none;background:none;color:#999;}
button::after{border: none;}
page{background:#f5f5f5;width:100%;height:100%;font-size: 24rpx;color:#333;font-family: "PingFang-SC-Medium"}
.disno{display: none}
